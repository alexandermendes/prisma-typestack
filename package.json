{
  "name": "prisma-typestack",
  "version": "1.3.0",
  "description": "Generates models from Prisma schemas using class-validator and class-transformer",
  "main": "index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.json .",
    "clean": "rimraf dist",
    "prebuild": "yarn run clean",
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "bin": {
    "prisma-typestack": "dist/index.js"
  },
  "author": "Alex Mendes",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexandermendes/prisma-typestack.git"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "prisma",
    "prisma-client",
    "prisma-schema",
    "prisma-generator",
    "class-validator",
    "class-transformer"
  ],
  "peerDependencies": {
    "@prisma/client": "^5",
    "class-transformer": "*",
    "class-validator": "*"
  },
  "dependencies": {
    "@prisma/generator-helper": "^5",
    "@types/node": "^18.15.11",
    "app-root-path": "^3.1.0",
    "fs-extra": "^11.1.1",
    "prettier": "^2.8.7",
    "typescript": "^5.0.3"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.4.4",
    "@prisma/client": "^5.6.0",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@types/fs-extra": "^11.0.1",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.57.0",
    "commitlint": "^8.3.5",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^4.2.5",
    "jest": "^29.5.0",
    "semantic-release": "^21.0.0"
  }
}
